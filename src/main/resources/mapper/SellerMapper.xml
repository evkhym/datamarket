<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="evan.khym.datamarket.repository.SellerRepo">

    <select id="findById" resultType="evan.khym.datamarket.model.DB.SellerDTO">
        SELECT * FROM seller WHERE ID = #{id}
    </select>
    <select id="findByName" resultType="evan.khym.datamarket.model.DB.SellerDTO">
        SELECT * FROM seller WHERE NAME = #{username}
    </select>


</mapper>